<div class="row">
    <div class="col-md-12">
        <h2 class="bold">{{name}}</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="card-columns">
            {{#each Items}}
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="{{imageUrl}}" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">{{name}}</h5>
                    <p class="card-text">{{description}}</p>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>